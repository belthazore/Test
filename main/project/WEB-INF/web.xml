<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD
  Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>

<web-app>

  <servlet>
    <servlet-name>gate</servlet-name>
    <servlet-class>Gate</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>auth</servlet-name>
    <servlet-class>AuthorizationChecker</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>

  <servlet>
    <servlet-name>lc</servlet-name>
    <servlet-class>LoginCheck</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>registration</servlet-name>
    <servlet-class>Registration</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>Home</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>servlet</servlet-name>
    <servlet-class>Servlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>gate</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registration</servlet-name>
    <url-pattern>/registration</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>

</web-app>